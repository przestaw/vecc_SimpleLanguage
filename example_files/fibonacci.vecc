fun fib_rec(n){
    if(n <= 1){
        return 1;
    } else {
        return fib_rec(n-1) + fib_rec(n-2);
    }
}

fun fib_it(n){
    var a = 0;
    if( n == 0){
        return a;
    }
    var b = 1;
    var c = a + b;

    while(0 < n){
        c = a + b;
        a = b;
        b = c;
        
        n = n - 1;
    }
    return b;
}